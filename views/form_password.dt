.mdl-textfield.mdl-js-textfield.mdl-textfield--floating-label
	input.mdl-textfield__input(type="password", id="password", name="password", data-required, pattern=".{8, 64}")
	label.mdl-textfield__label(for="password") password*
	span.mdl-textfield__error password required
	.mdl-tooltip.mdl-tooltip--large(data-mdl-for="password") pattern ".{8,64}"